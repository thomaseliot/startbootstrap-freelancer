FormatVersion=5.0 // Do not edit this line!
Title="CMR16e"

{ENUMS}
enum State(0="GLV_ON", // GLV On
  1="HV_EN", // HV Enable
  2="RTD", // Ready To Drive
  3="ERROR")// Error
enum Pump_State(1="On", 0="Off")
enum BMS_Precharge_State(0="Error", 1="Idle", 2="Measure", 3="Precharge", 
  4="Run", 5="Enable Pack")
enum BMS_Precharge_Timer_State(0="Timer Not Elapsed", 1="Timer Elapsed")

{SENDRECEIVE}

[FSM_Heartbeat]
ID=202h	// Front Sensor Module Heartbeat
DLC=4
CycleTime=10
Timeout=100
Var=State State 0,8	// Current Module State
Var=VBatt unsigned 8,8 /u:V /f:0.066666
Var=Requested_Torque unsigned 16,8
Var=Speed unsigned 24,8 /u:MPH

[DIM_Heartbeat]
ID=204h	// Driver Interface Module Hearbeat
DLC=3
CycleTime=10
Timeout=100
Var=State State 0,8
Var=Requested_State State 16,8
Var=VBatt unsigned 8,8 /u:V /f:0.066666

[CC_Heartbeat]
ID=201h
DLC=2
CycleTime=10
Timeout=100
Var=State State 0,8
Var=VBatt unsigned 8,8 /u:V /f:0.066666

[AFC_Heartbeat]
ID=205h
DLC=0
CycleTime=10
Timeout=100
Var=State State 0,8
Var=AFC_VBatt unsigned 8,8 /u:V /f:0.066666
Var=Fan_1_Current unsigned 16,8 /u:A
Var=Fan_2_Current unsigned 24,8 /u:A
Var=Fan_3_Current unsigned 32,8 /u:A

[TM_Heartbeat]
ID=206h
DLC=2
CycleTime=10
Timeout=100
Var=State State 0,8
Var=VBatt unsigned 8,8 /u:V /f:0.066666

[RSM_Heartbeat]
ID=203h
DLC=5
CycleTime=10
Timeout=100
Var=State State 0,8
Var=VBatt unsigned 8,8 /f:0.066666
Var=Fan_Current unsigned 16,8 /u:A
Var=Pump_L_Status Pump_State 24,8
Var=Pump_R_Status Pump_State 32,8

[SM_Heartbeat]
ID=200h
DLC=3
CycleTime=10
Timeout=100
Var=State State 0,8
Var=SM_Vbatt unsigned 8,8 /u:V /f:0.066666
Var=Target_State State 16,8

[MC_L_Modulation_And_Flux_Info]
ID=3ADh
DLC=8
Var=D4_Iq_Command signed 48,16 /u:A /f:0.1 /max:3276.7	// The commanded Q-axis current
Var=D3_Id_Command signed 32,16 /u:A /f:0.1 /max:3276.7	// The commanded D-axis current
Var=D2_Flux_Weakening_Output signed 16,16 /u:A /f:0.1 /max:3276.7	// This is the current output of the flux regulator.
Var=D1_Modulation_Index signed 0,16 /f:0.0001	// This is the modulation index. The scale factor is x100. To get the actual modulation index divide the value by 100.

[MC_L_Torque_And_Timer_Info]
ID=3ACh
DLC=8
CycleTime=10
Var=D3_Power_On_Timer unsigned 32,32 /u:Sec /f:0.003	// Updated every 3 msec. This will roll over in approximately 150 days!
Var=D2_Torque_Feedback signed 16,16 /u:Nm /f:0.1 /max:3276.7	// Estimated motor torque feedback
Var=D1_Commanded_Torque signed 0,16 /u:Nm /f:0.1 /max:3276.7	// The commanded Torque

[MC_L_Read_Write_Param_Response]
ID=3C2h	// To write a parameter use message 0x0C1 with byte #2 set to 1 (write). To read a parameter use message 0x0C1 with byte #2 to set 0 (read). 
DLC=8
CycleTime=10
Var=D2_Write_Success bit 16,1	// 0=Write failure, 1=Success
Var=D3_Data_Response signed 32,16	// All EEPROM data is 16 bits and is contained in bytes 4 and 5. Bytes 6 and 7 should be ignored.
Var=D1_Parameter_Address_Response unsigned 0,16	// Valid EEPROM Parameter CAN addresses are between 100 and 499.

[MC_L_Read_Write_Param_Command]
ID=3C1h	// To write a parameter use message 0x0C1 with byte #2 set to 1 (write). To read a parameter use message 0x0C1 with byte #2 to set 0 (read). 
DLC=8
CycleTime=10
Var=D3_Data_Command signed 32,16	// All EEPROM data is 16 bits and is contained in bytes 4 and 5.
Var=D2_Read_Write_Command bit 16,1	// 0=Read, 1=Write
Var=D1_Parameter_Address_Command unsigned 0,16	// Valid EEPROM Parameter CAN addresses are between 100 and 499.

[MC_L_Command_Message]
ID=3C0h	// The command message is used to transmit data to the controller. This message is sent from a user supplied external controller to the PMxxx controller.
DLC=8
CycleTime=5
Var=Inverter_Enable bit 40,1 /u:Bit	// 0=Inverter OFF, 1 = Inverter ON
Var=Direction_Command bit 32,1 /u:Bit	// 0=CW, 1=CCW as veiwed from the shaft end of the motor
Var=Speed_Command signed 16,16 /u:rpm	// Speed commandused when in speed mode
Var=Torque_Command signed 0,16 /u:Nm /f:0.1 /max:3276.7	// Torque command when in torque mode
Var=Inverter_Discharge bit 41,1 /u:Bit	// 0=Discharge Disable,=Discharge Enable
Var=Torque_Limit_Command signed 48,16 /u:Nm /min:-3276.8 /max:3276.7	// Torque Limit, set to 0 to keep default
Var=Speed_Mode_Enable bit 42,1 /u:Bit	// 0 = No change to mode, 1 = change to speed mode from torque mode

